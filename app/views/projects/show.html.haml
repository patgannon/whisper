
.field
  .fieldName Name:
  .fieldValue= @project.name
.field
  .fieldName Description:
  .fieldValue= @project.description
  
#domain_names
  Web site names for this project:
  %ul.domain_names
    - @project.domain_names.each do |domain_name|
      %li
        = domain_name.domain_name
        = link_to 'Destroy', project_domain_name_path(@project, domain_name), :confirm => 'Are you sure?', :method => :delete if can? :delete, domain_name

= link_to 'Edit', edit_project_path(@project) if can? :edit, Project
\|
= link_to 'Back', projects_path
